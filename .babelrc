{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["babel-plugin-module-resolver", {
      "root": ["./src"],
      "alias": {
        "@enums": "./src/enums",
        "@packets": "./src/packets",
        "@structs": "./src/structs",
        "@managers": "./src/managers",
        "@core": "./src"
      }
    }]
  ]
}
